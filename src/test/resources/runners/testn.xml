<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression PW Suite" verbose="4" thread-count="2" parallel="tests">
    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter2"/>
    </listeners>


    <test name="Open Cart Home Page Test_chrome">
        <classes>
            <class name="com.opencart.tests.HomePageTest"/>
        </classes>
    </test>

    <test name="Open Cart Login Page Test_chrome">
        <classes>
            <class name="com.opencart.tests.LoginPageTest"/>
        </classes>
    </test>

</suite>